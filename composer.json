{
    "name": "devboard/github-doctrine-types-bundle",
    "license": "proprietary",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "comment": "Adding my own multiline fixers",
            "url": "https://github.com/msvrtan/PHP-CS-Fixer.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "DevboardLib\\GitHubDoctrineTypeBundle\\": "src/"
        }
    },
    "require": {
        "php":                                      ">=7.1.0",
        "devboard/github-doctrine-types":           "^2.0",
        "symfony/http-kernel": "^3.3|^4",
        "devboard/lib-git": "^2.0",
        "devboard/git-interfaces": "^2.0",
        "devboard/lib-github": "^2.0",
        "devboard/lib-generix": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "dev-long-line-fixers",
        "phing/phing": "^2.16",
        "phpmd/phpmd": "^2.6",
        "phpstan/phpstan": "^0.10.1",
        "slevomat/coding-standard": "^5.0",
        "squizlabs/php_codesniffer": "^3.1",
        "phpstan/phpstan-phpunit": "^0.10.0",
        "infection/infection": "^0.13.3",
        "phpstan/phpstan-strict-rules": "^0.10.0",
        "phpstan/phpstan-deprecation-rules": "^0.10.2",
        "phpstan/phpstan-mockery": "^0.10.1"
    },
    "config": {
        "platform": {
            "php": "7.1.3"
        }
    }
}
