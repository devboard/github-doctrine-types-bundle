{
    "name": "devboard/github-doctrine-types-bundle",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "DevboardLib\\GitHubDoctrineTypeBundle\\": "src/",
            "tests\\DevboardLib\\GitHubDoctrineTypeBundle\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\DevboardLib\\GitHubDoctrineTypeBundle\\": "spec/"
        }
    },
    "require": {
        "php":                                      ">=7.1.0",
        "devboard/github-doctrine-types":           "^1.0",
        "symfony/http-kernel": "^3.3"
    },
    "require-dev": {
        "phpmd/phpmd":                              "~2.1",
        "squizlabs/php_codesniffer":                "^3.0",
        "friendsofphp/php-cs-fixer":                "^2.0",
        "phing/phing":                              "^2.15",
        "phpstan/phpstan":                          "^0.7.0"
    },
    "config": {
        "platform": {
            "php": "7.1.0"
        },
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev"
        }
    }
}
